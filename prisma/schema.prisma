generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model acomodacao {
  id          String @id @default(cuid())
  name        String
  type        String
  country     String
  value       String
  image       String
  description String
  avaliation  String?
}

model reserva {
  id             String @id @default(cuid())
  checkin        String
  checkout       String
  idAcomodacao   String
  idUsuario      String
  nomeAcomodacao String
  preco          String
}

model usuario {
  id    String @id @default(uuid())
  nome  String
  email String @unique
  senha String
}
